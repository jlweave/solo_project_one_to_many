<%= form_with url: "/books/#{@book.id}", method: :patch, local: true do |form|%>
<p><%= form.label :name %></p>
<p><%= form.text_field :name %></p>
<p><%= form.label :IBN %></p>
<p><%= form.text_field :IBN %></p>
<p>Is Available?<p>
<p>true<%= form.radio_button :available, value=true%></p>
<p>false<%= form.radio_button :available, value=false%></p>
<%= form.submit 'Update Book' %>
<% end %>